{
    "name": "quantaforge/validation",
    "description": "The QuantaForge Validation package.",
        "license": "proprietary",
    "authors": [
        {
            "name": "Tomáš Selič (Smisch-DEV)",
            "email": "contact@smisch.sk"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-filter": "*",
        "ext-mbstring": "*",
        "brick/math": "^0.9.3|^0.10.2|^0.11",
        "egulias/email-validator": "^3.2.5|^4.0",
        "quantaforge/collections": "^1.0.0",
        "quantaforge/container": "^1.0.0",
        "quantaforge/contracts": "^1.0.0",
        "quantaforge/macroable": "^1.0.0",
        "quantaforge/support": "^1.0.0",
        "quantaforge/translation": "^1.0.0",
        "symfony/http-foundation": "^6.2",
        "symfony/mime": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "QuantaForge\\Validation\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.x-dev"
        }
    },
    "suggest": {
        "quantaforge/database": "Required to use the database presence verifier (^10.0)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
